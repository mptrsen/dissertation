  url = {http://www.pnas.org/content/110/36/14705},
  journal = {Proceedings of the National Academy of Sciences},
  author = {Remnant, Emily J. and Good, Robert T. and Schmidt, Joshua M. and Lumb, Christopher and Robin, Charles and Daborn, Phillip J. and Batterham, Philip},
  month = sep,
  year = {2013},
  pages = {14705-14710},
  file = {/home/malty/.mozilla/firefox/uhivughd.default/zotero/storage/DSVDVF4V/Remnant et al. - 2013 - Gene duplication in the major insecticide target s.pdf;/home/malty/.mozilla/firefox/uhivughd.default/zotero/storage/Z3RHXWXZ/14705.html},
  pmid = {23959864}
}

@article{Mathiopoulos1998,
